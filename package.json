{
  "name": "mailazy-node",
  "description": "Mailazy NodeJs SDK Client",
  "version": "0.0.1",
  "author": "Kunal <kunal.saini@mailazy.com> ()",
  "bugs": {
    "url": "https://github.com/mailazy/mailazy-node/issues",
    "email": "kunal.saini@mailazy.com"
  },
  "contributors": [
    "Kunal <kunal.saini@mailazy.com> ()"
  ],
  "dependencies": {
    "http2": "^3.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "codecov": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "fixpack": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "remark-cli": "latest",
    "remark-preset-github": "latest",
    "xo": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "homepage": "https://github.com/mailazy/mailazy-node",
  "keywords": [
    "mailazy",
    "sdk"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mailazy/mailazy-node"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "xo",
    "lint:md": "remark . -qfo",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test ava",
    "test-coverage": "cross-env NODE_ENV=test nyc npm run test"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
